{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "image-carousel-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/imageCarousel/ImageCarouselWebPart.js",
          "manifest": "./src/webparts/imageCarousel/ImageCarouselWebPart.manifest.json"
        }
      ]
    },
    "forms-and-templates-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/formsAndTemplates/FormsAndTemplatesWebPart.js",
          "manifest": "./src/webparts/formsAndTemplates/FormsAndTemplatesWebPart.manifest.json"
        }
      ]
    },
    "birthday-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/birthday/BirthdayWebPart.js",
          "manifest": "./src/webparts/birthday/BirthdayWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ImageCarouselWebPartStrings": "lib/webparts/imageCarousel/loc/{locale}.js",
    "FormsAndTemplatesWebPartStrings": "lib/webparts/formsAndTemplates/loc/{locale}.js",
    "BirthdayWebPartStrings": "lib/webparts/birthday/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}